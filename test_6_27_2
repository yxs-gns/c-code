#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include <stdlib.h>
int main()
{
	int arr[]={1,2,5,8,9,6,3,2,5,7,8,9,22,66,88,4,6};
	int k=7;
	int i=0;
	int sz=sizeof(arr)/sizeof(arr[0]);
	for(i=0;i<sz;i++)
	{
		if(k==arr[i])
		{
			printf("找到了，下标是%d\n",i);
			break;
		}
		if(i==sz)
			printf("找不到\n");
	}
	system("pause");
	return 0;
}
